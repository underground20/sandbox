{
    "name": "undeground20/sandbox",
    "description": "Sandbox with examples of patterns and other things",
    "license": ["MIT"],
    "version": "0.1",
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "require": {
        "prewk/result": "^3.1",
        "php-ds/php-ds": "^1.5",
        "spiral/roadrunner": "^2025.1",
        "psr/http-message": "^2.0",
        "nyholm/psr7": "^1.8",
        "spiral/roadrunner-http": "^3.6",
        "spiral/roadrunner-cli": "^2.7"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^11.1",
        "phpstan/phpstan": "^1.11"
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
